<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gokisoft.example">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".NewNoteDetailActivity"
            android:exported="false" />
        <activity
            android:name=".NewNoteActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NoteActivity"
            android:exported="true"></activity>

        <service
            android:name=".CounterService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".ServicesActivity"
            android:exported="true" />
        <activity
            android:name=".BookEditorActivity"
            android:exported="false" />
        <activity
            android:name=".BookActivity"
            android:exported="true" />
        <activity android:name=".MainActivity" />
        <activity android:name=".DetailActivity" />
        <activity android:name=".SubjectActivity" />
        <activity android:name=".InputSubjectActivity" />
        <activity android:name=".StudentActivity" />
        <activity android:name=".EditorStudentActivity" />
        <activity android:name=".NewStudentActivity" />
        <activity android:name=".DetailStudentActivity" />
    </application>

</manifest>